{
	// !!! Some options are better to be left commented! They will overwrite the command-line parameters otherwise!

	// Set the name of the .log file:
	//"log_name":"PackMakerLite",

	// Set the prefix of the cs .dat file:
	//"cs_name":"",

	// ignore the following specific filenames
	"ignore_full_name": [".git", ".svn", ".vs", "desktop.ini"],

	// ignore these specific file extensions
	"ignore_file_extension": ["bak", "db", "wdp"],

	// convert every FiLeName to lower case 'filename'
	"force_filename_lowercase": false,

	// use this pack type if no rule in pack_type_by_list matches
	"default_pack_type": 2,

	// specify your pack types based on the file extensions
	"pack_type_by_list": [
		/*commented
		["mss", 0],
		["mse", 1],
		["msf", 2],
		["spt", 0],
		["atr", 1],
		["dds", 2],
		["raw", 0],
		["wtr", 1],
		["mde", 2],
		["tga", 0],
		["txt", 1],
		["msk", 2],
		["msa", 0],
		["msm", 1],
		["py", 2]*/
	],

	// use parallel or --parallel
	//"parallel":true,

	// pack the hidden files (.XXX or invisible) files if true (--hiddenfiles is true, --skipignorefiles is false)
	//"hidden_files":true,

